{
  "name": "Unreal Speccy Portable - ZX Spectrum emulator",
  "short_name": "USP",
  "version": "0.0.81",
  "manifest_version": 2,
  "description": "Portable ZX Spectrum emulator supports Z80 128K (Pentagon) AY/YM, Beeper, BetaDisk, Tape, Kempston Joystick, Snapshots, Replays.",
  "icons": {
    "16" : "unreal_speccy_portable_16.png",
    "48" : "unreal_speccy_portable_48.png",
    "128": "unreal_speccy_portable_128.png"
  },
  "app": {
    "launch": {
      "local_path": "unreal_speccy_portable.html"
    }
  },
  "permissions": [
	"https://vtrd.in/",
	"http://rzxarchive.co.uk/",
	"http://bbb.retroscene.org/"
  ],
  "requirements": {
    "3D": {
    	"features": ["webgl"]
  	}
  },
  "minimum_chrome_version": "49",
  "homepage_url": "https://bitbucket.org/djdron/unrealspeccyp"
}
